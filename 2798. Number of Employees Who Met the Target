class Solution {
    public int numberOfEmployeesWhoMetTarget(int[] hours, int target) {
        int len=hours.length;
        int length=0;
        for(int i=0;i<=len-1;i++){
            if(hours[i]>=target){
                length++;
            }
        }
        if(length==0){
            return 0;
        }
        return length;
    }
}
